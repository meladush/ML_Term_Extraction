\\u0438\\u044f \\u0438 \\u0441\\u0443\\u0442\\u044c \\u0435\\u0435 \\u043a\\u043e\\u043c\\u043f\\u043e\\u043d\\u0435\\u043d\\u0442\\u043e\\u0432.\"\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"label\",\n      \"properties\": {\n        \"dtype\": \"object\",\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"terms\",\n      \"properties\": {\n        \"dtype\": \"object\",\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"new_spans\",\n      \"properties\": {\n        \"dtype\": \"object\",\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    }\n  ]\n}"}},"metadata":{},"execution_count":13}],"source":["df"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"MD0MNi3K1kCm"},"outputs":[],"source":["def get_anti_spans(x):\n","\n","  upd_spans = x['new_spans']\n","  text = x['text']\n","\n","  anti_spans = []\n","\n","  if upd_spans[0][0] != 0:\n","    anti_spans.append([0, upd_spans[0][0]])\n","\n","  for i, span in enumerate(upd_spans[:-1]):\n","    current, next = upd_spans[i], upd_spans[i + 1]\n","    if current[-1] != next[0] - 1:\n","      anti_spans.append([current[-1], next[0]])\n","\n","  if upd_spans[-1][-1] != len(text):\n","    anti_spans.append([upd_spans[-1][-1], len(text)])\n","\n","  try:\n","    for i in anti_spans: # удаляем пробелы (но не \\n) в начале и в конце, если они есть, иначе не формируется объект Span\n","      if text[i[0]] == ' ':\n","        i[0] = i[0] + 1\n","      if text[i[1] - 1] == ' ':\n","        i[1] = i[1] - 1\n","  except:\n","    spans = None\n","\n","  return anti_spans"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"EftNaTDf5kgP"},"outputs":[],"source":["df['anti_spans'] = df.apply(get_anti_spans, axis=1)"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":